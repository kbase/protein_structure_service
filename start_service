#!/bin/sh
echo starting KBaseProteinStructure service.
export PERL5LIB=$PERL5LIB:/kb/deployment/lib
export KB_DEPLOYMENT_CONFIG=/kb/dev_container/modules/protein_structure_service/deploy.cfg
export KB_SERVICE_NAME=KBaseProteinStructure
#uncomment to debug: export STARMAN_DEBUG=1
/kb/runtime/bin/starman --listen :7088 --pid /kb/deployment/services/KBaseProteinStructure/service.pid --daemonize \
  --access-log /kb/deployment/services/KBaseProteinStructure/log/access.log \
  --error-log /kb/deployment/services/KBaseProteinStructure/log/error.log \
  /kb/deployment/lib/KBaseProteinStructure.psgi
echo KBaseProteinStructure service is listening on port 7088.

